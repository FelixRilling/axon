{
    "name": "axonjs",
    "version": "2.1.0",
    "description": "A super tiny vue-like framework",
    "main": "dist/axon.common.js",
    "module": "dist/axon.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "Axon",
                "file": "axon"
            },
            "targets": {
                "browsers": "chrome >= 58"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/axonjs.git"
    },
    "keywords": [
        "framework",
        "directives",
        "vue-like"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/axonjs/issues"
    },
    "homepage": "https://github.com/FelixRilling/axonjs#readme",
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-plugin-external-helpers": "^6.22.0",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.11.0",
        "npm-run-all": "^4.1.2",
        "rollup": "^0.51.8",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "uglify-es": "^3.1.10"
    },
    "dependencies": {
        "lightdash": "^3.2.0",
        "pseudo-eval": "^6.0.0"
    }
}
